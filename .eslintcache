);\r\n      const response = await axios.post(\r\n        'http://localhost:8000/common/register',\r\n        {\r\n          email,\r\n          password,\r\n        }\r\n      );\r\n      setInvalid('');\r\n      setShowInfo(true);\r\n    } catch (e) {\r\n      const error = e.response.data || e.message;\r\n      setInvalid(error);\r\n    } finally {\r\n      setRegistering(false);\r\n    }\r\n  };\r\n\r\n  const forgotPassword = () => {\r\n    console.log('forgot password');\r\n  };\r\n\r\n  const confirm = (e) => {\r\n    e.preventDefault();\r\n    console.log('confirming!');\r\n  };\r\n\r\n  return registering ? (\r\n    <Registering />\r\n  ) : (\r\n    <div className='login'>\r\n      {showInfo ? (\r\n        <>\r\n          <h1>Before confirming your appointment</h1>\r\n          <p>We need a little info</p>\r\n          <form id='form-reg-info' className='form' onSubmit={confirm}>\r\n            <label htmlFor='name'>Full Name</label>\r\n            <input\r\n              autoFocus\r\n              type='text'\r\n              name='name'\r\n              value={name}\r\n              onChange={(e) => setName(e.target.value)}\r\n            />\r\n            <label htmlFor='phone'>Phone</label>\r\n            <input\r\n              type='tel'\r\n              name='phone'\r\n              value={phone}\r\n              onChange={(e) => setPhone(e.target.value)}\r\n            />\r\n            <label htmlFor='dob'>Date Of Birth</label>\r\n            <input\r\n              type='date'\r\n              name='dob'\r\n              value={date}\r\n              onChange={(e) => setDate(e.target.value)}\r\n            />\r\n            <button type='submit'>Confirm</button>\r\n          </form>\r\n        </>\r\n      ) : signup ? (\r\n        <>\r\n          <h1>Create an account with CityMD</h1>\r\n          <p>Book appointments smoothly</p>\r\n        </>\r\n      ) : (\r\n        <h1>Login to your account</h1>\r\n      )}\r\n      {invalid && <h2>{invalid